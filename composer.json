{
    "name": "doyo/code-coverage-bridge",
    "description": "Add a functionality to customize phpunit/php-code-coverage",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Anthonius Munthi",
            "email": "me@itstoni.com"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev",
            "dev-develop": "2.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Doyo\\Bridge\\CodeCoverage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\Doyo\\Bridge\\CodeCoverage\\": "spec/"
        }
    },
    "require": {
        "php": ">=7.0",
        "phpunit/php-code-coverage": "^5.3 | ^6.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^4.3 | ^5.0",
        "symfony/http-foundation": "^3.4 | ^4.3",
        "symfony/cache": "^3.4"
    }
}
